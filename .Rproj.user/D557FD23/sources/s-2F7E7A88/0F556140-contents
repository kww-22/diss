library(tidyverse) 
path <- getwd()
data <- read.csv(str_glue(path,'/sup/velo_csvs/data_warm.csv'))

ggplot(data = data, aes(x=Throw, y=Vel_warm, color = pID)) +
  geom_point(size = 4) +
  theme_bw() +
  theme(aspect.ratio = 1,
        axis.text = element_text(size = 20),
        axis.title = element_text(size = 20)) +
  ylab('Velo')
